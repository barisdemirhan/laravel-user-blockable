{
  "name": "barisdemirhan/laravel-user-blockable",
  "description": "Laravel 5 user block system",
  "keywords": [
    "laravel",
    "block"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "hareku",
      "email": "hareku908@gmail.com"
    },
    {
      "name": "barisdemirhan",
      "email": "barisdemirhn@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/database": ">=5.4",
    "illuminate/support": ">=5.4",
    "nesbot/carbon": ">=2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "Hareku\\LaravelBlockable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hareku\\LaravelBlockable\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "repositories": {
    "laravel-user-blockable": {
      "type": "vcs",
      "url": "https://github.com/barisdemirhan/laravel-user-blockable.git"
    }
  }
}
